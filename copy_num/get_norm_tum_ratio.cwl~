cwlVersion: v1.0
class: CommandLineTool
baseCommand: ['usr/bin/python', 'get_tum_norm_ratio.py']
label: "Get normal:tumor        ratio"                 
inputs:
    normal_bam:
        type: File
    tumor_bam:
        type: File
    data_ratio:
        type: string?
        default: None
output:
    tum_norm_ratio:
        type: File
        outputBinding:
            glob: "output.ratio"